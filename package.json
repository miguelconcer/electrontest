{
  "name": "AsinBot_app",
  "version": "1.0.0",
  "description": "AsinBot",
  "productName": "AsinBot",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "build --win",
    "ship": "build --win -p always",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . AsinBot-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ASIN Assistant\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "Miguel Contreras",
  "license": "MIT",
  "repository": "https://github.com/miguelconcer/electrontest",
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-packager": "^9.1.0"
  }
}
